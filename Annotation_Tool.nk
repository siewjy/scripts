  addUserKnob {22 pasteNode l "Replace Placholder" T "def replacePlaceholder():\n    pNode.setXpos(refNode.xpos())\n    pNode.setYpos(refNode.ypos()-7)\n\n    #Connect Annotation Tool\n    checkTask = refNode\['label'].value()\n\n    if checkTask == \"PLACEHOLDER - ROTO\":\n\n        #Roto\n        pNode.setInput(0, dot)\n        pNode.setInput(1, rp_red)\n        pNode.setInput(2, rp_green)\n        pNode.setInput(3, rp_blue)\n        pNode.setInput(5, rp_white)\n        switcher_name = \"!Annotation_Switcher.\"\n        link = switcher_name+new_Roto\n        nuke.message(checkTask + \" replaced!\")\n\n    \n    elif checkTask == \"PLACEHOLDER - PAINT\":\n\n        #Paint\n        pNode.setInput(4, rp_paint)\n        pNode.setInput(5, rp_white)\n        switcher_name = \"!Annotation_Switcher.\"\n        link = switcher_name+new_Paint\n        nuke.message(checkTask + \" replaced!\")\n\n\n    #Linking to Annotation Switcher\n    pNode\['disable'].setExpression(link)\n\n\n#Paste with Checker\n\nrefNode = nuke.selectedNode()\nut = nuke.thisNode()\n\nif  not refNode.knob('Placeholder'):\n    nuke.message('Please Select the <b>Placeholder</b> node!')\n    \nelse:\n    #Reset Copy\n    ut\['rNode'].setValue(\"\")\n    pNode = nuke.nodePaste('%clipboard%')\n    replacePlaceholder()\n    \n    #Deleting Placeholder\n    nuke.delete(refNode)" +STARTLINE}
