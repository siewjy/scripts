  addUserKnob {22 paintPresets l "<b>Update Paint Presets" T "n = nuke.thisNode()\n\np_preset0 = n\['preset_paint_01'].value()\np_preset1 = n\['preset_paint_02'].value()\np_preset2 = n\['preset_paint_03'].value()\np_preset3 = n\['preset_paint_04'].value()\np_preset4 = n\['preset_paint_05'].value()\np_preset5 = n\['preset_paint_06'].value()\n\ninputCustom1 = n\['custom_01'].value()\ninputCustom2 = n\['custom_02'].value()\ninputCustom3 = n\['custom_03'].value()\ninputCustom4 = n\['custom_04'].value()\ninputCustom5 = n\['custom_05'].value()\ninputCustom6 = n\['custom_06'].value()\ninputCustom7 = n\['custom_07'].value()\ninputCustom8 = n\['custom_08'].value()\ninputCustom9 = n\['custom_09'].value()\ninputCustom10 = n\['custom_10'].value()\n\ndef set_Paint_Presets():\n\n    for a in nuke.allNodes(\"Group\"):\n        task = a.knob(\"task_dropdown\")\n        if not task:\n            continue\n        \n        if task.value() == \"Paint\":\n            #a\['note_font_size'].setValue(15)\n            #Updating the Lists\n            a\['paint_presets'].setValues(\[p_preset0,p_preset1,p_preset2,p_preset3,p_preset4,p_preset5,inputCustom1,inputCustom2,inputCustom3,inputCustom4,inputCustom5,inputCustom6,inputCustom7,inputCustom8,inputCustom9,inputCustom10])\n\n    #Prompt to show Updated Presets\n    nuke.message(p_preset1+\"\\n\"+p_preset2+\"\\n\"+p_preset3+\"\\n\"+p_preset4+\"\\n\"+p_preset5+'\\n'+'\\n'+\"are updated as Presets\")\n        \nset_Paint_Presets()" +STARTLINE}
