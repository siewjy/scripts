  addUserKnob {22 create_newTree_Roto l "<b>Create New Annotation (Roto)" T "n = nuke.thisNode()\n\n#Check if All Branches are used up\nswitcher = nuke.toNode(\"Annotation_Switcher\")\nif switcher\['rotoCounter'].value() == 1:\n    \n    r_marker = nuke.toNode(\"r_Marker\") \n\n    dot = nuke.createNode('Dot', inpanel=False)\n    dot.setXpos(r_marker.xpos()+300)\n    dot.setYpos(r_marker.ypos()+0)\n    dot.setInput(0, r_marker)\n    \n    connector = nuke.createNode('Dot', inpanel=False)\n    connector.setXpos(r_marker.xpos()+500)\n    connector.setYpos(r_marker.ypos()+0)\n    connector.setInput(0, dot)\n    \n    \n    add = nuke.createNode('StickyNote', inpanel=False)\n    add.setXpos(r_marker.xpos()+315)\n    add.setYpos(r_marker.ypos()+15)\n    add.knob('tile_color').setValue(0x777777ff)\n    add.knob('note_font_size').setValue(20)\n    \n    # Placeholder\n    ph = nuke.createNode('Group', inpanel=False)\n    ph.knob('label').setValue(\"PLACEHOLDER - ROTO\")\n    ph.knob('tile_color').setValue(0xaa55ffff)\n    ph.setXpos(r_marker.xpos()+270)\n    ph.setYpos(r_marker.ypos()+256)\n    x_check = nuke.Boolean_Knob(\"Placeholder\", \"Placeholder\", True)\n    ph.addKnob(x_check)\n    \n    nuke.toNode(ph.knob('name').value()).begin()\n    ph_01 = nuke.createNode('Input', inpanel=False)\n    ph_01.knob('name').setValue(\"Plate\")\n    ph_02 = nuke.createNode('Input', inpanel=False)\n    ph_02.knob('name').setValue(\"Red\")\n    ph_03 = nuke.createNode('Input', inpanel=False)\n    ph_03.knob('name').setValue(\"Green\")\n    ph_04 = nuke.createNode('Input', inpanel=False)\n    ph_04.knob('name').setValue(\"Blue\")\n    ph_06 = nuke.createNode('Input', inpanel=False)\n    ph_06.knob('name').setValue(\"Description\")\n    ph_05 = nuke.createNode('Input', inpanel=False)\n    ph_05.knob('name').setValue(\"Paint\")\n    nuke.createNode('Output', inpanel=False)\n    ph.end()\n    \n    # RotoPaint nodes\n    rp_red = nuke.createNode('RotoPaint', inpanel=False)\n    rp_red.setXpos(r_marker.xpos()+100)\n    rp_red.setYpos(r_marker.ypos()+150)\n    rp_red.setInput(0, None)\n    rp_red.knob('tile_color').setValue(0xff0000ff)\n    rp_red.knob('label').setValue(\"ROTO RED\")\n    rp_red.knob('output').setValue(\"alpha\")\n    rp_red.knob('lifetime_type').setValue(\"all frames\")\n    \n    rp_green = nuke.createNode('RotoPaint', inpanel=False)\n    rp_green.setXpos(r_marker.xpos()+100)\n    rp_green.setYpos(r_marker.ypos()+250)\n    rp_green.setInput(0, None)\n    rp_green.knob('tile_color').setValue(0xff00ff)\n    rp_green.knob('label').setValue(\"ROTO GREEN\")\n    rp_green.knob('output').setValue(\"alpha\")\n    rp_green.knob('lifetime_type').setValue(\"all frames\")\n    \n    rp_blue = nuke.createNode('RotoPaint', inpanel=False)\n    rp_blue.setXpos(r_marker.xpos()+100)\n    rp_blue.setYpos(r_marker.ypos()+330)\n    rp_blue.setInput(0, None)\n    rp_blue.knob('tile_color').setValue(0xffff)\n    rp_blue.knob('label').setValue(\"ROTO BLUE\")\n    rp_blue.knob('output').setValue(\"alpha\")\n    rp_blue.knob('lifetime_type').setValue(\"all frames\")\n    \n    rp_white = nuke.createNode('RotoPaint', inpanel=False)\n    rp_white.setXpos(r_marker.xpos()+100)\n    rp_white.setYpos(r_marker.ypos()+410)\n    rp_white.setInput(0, None)\n    rp_white.knob('tile_color').setValue(0xffffffff)\n    rp_white.knob('label').setValue(\"DESCRIPTION\")\n    rp_white.knob('output').setValue(\"alpha\")\n    rp_white.knob('lifetime_type').setValue(\"all frames\")\n    \n    \n    dot_end = nuke.createNode('Dot', inpanel=False)\n    dot_end.setXpos(r_marker.xpos()+300)\n    dot_end.setYpos(r_marker.ypos()+720)\n    dot_end.setInput(0, ph)\n    \n    #Connect Placeholder\n    ph.setInput(0, dot)\n    ph.setInput(1, rp_red)\n    ph.setInput(2, rp_green)\n    ph.setInput(3, rp_blue)\n    #ph.setInput(5, paint)\n    ph.setInput(4, rp_white)\n    \n    #r_marker.knob('name').setValue(\"old_Marker\")\n    #dot.knob('name').setValue(\"R_Marker\")\n    \n    #Counting amount of exisiting and additional Annotation added\n    switcher = nuke.toNode(\"Annotation_Switcher\")\n    r_count = int(switcher\['rotoCounter'].value())+int(switcher\['a_rCounter'].value())\n    \n    new_Roto_no = int(r_count + 1)\n    new_Roto_name = \"roto\"\n    label = \"Roto \"\n    new_Roto = new_Roto_name + str(new_Roto_no)\n    new_Roto_label = label + str(new_Roto_no)\n    new = nuke.Boolean_Knob(new_Roto, new_Roto_label, True)\n    \n    #Add new Knob\n    switcher.addKnob(new)\n    rotoCounter_no = int(switcher\['rotoCounter'].value())\n    a_rotoCounter = new_Roto_no-rotoCounter_no\n    switcher\['a_rCounter'].setValue(a_rotoCounter)\n    \n    #Linking to Append Clip and Contact Sheets\n    total_count = int(switcher\['total_Counter'].value())-1\n    total_roto_count = new_Roto_no-1\n    \n    appendClip = nuke.toNode(\"AppendClip\")\n    appendClip.setInput(total_count, dot_end)\n    \n    contactSheet_roto = nuke.toNode(\"SmartContactSheet_roto\")\n    contactSheet_roto.setInput(total_roto_count, dot_end)\n    \n    dot_suffix = \"_DOT\"\n    dot_name = new_Roto + dot_suffix \n    dot_end\['name'].setValue(dot_name)\n    \n    #Setting Sticky Note Label Name\n    add.knob('label').setValue(\"Additional- \"+new_Roto_label)\n    \n    #Update Sort Order Value\n    old_sortOrder = switcher\['sortOrder'].value()\n    new_sortOrder = new_Roto + \",\" + old_sortOrder\n    switcher\['sortOrder'].setValue(new_sortOrder)\n    \n    #Renaming r_Marker\n    r_marker_suffix = 'r_Marker_old'\n    r_marker_no = str(total_count)\n    r_marker_old = r_marker_suffix + r_marker_no\n    r_marker\['name'].setValue(r_marker_old)\n    \n    connector\['name'].setValue('r_Marker')\n    \n    del r_count\n    del new_Roto_no\n    del a_rotoCounter\n    \n    del connector\n    del r_marker\n    \nelse:\n    nuke.message(\"There are still Roto Annotation Branches available!\")" +STARTLINE}
  addUserKnob {22 create_newTree_Paint l "<b>Create New Annotation (Paint)" -STARTLINE T "n = nuke.thisNode()   \n\n#Check if All Branches are used up\nswitcher = nuke.toNode(\"Annotation_Switcher\")\nif switcher\['paintCounter'].value() == 1:\n    \n    r_marker = nuke.toNode(\"r_Marker\")\n    \n    dot = nuke.createNode('Dot', inpanel=False)\n    dot.setXpos(r_marker.xpos()+300)\n    dot.setYpos(r_marker.ypos()+0)\n    dot.setInput(0, r_marker)\n    \n    connector = nuke.createNode('Dot', inpanel=False)\n    connector.setXpos(r_marker.xpos()+500)\n    connector.setYpos(r_marker.ypos()+0)\n    connector.setInput(0, dot)\n    \n    \n    add = nuke.createNode('StickyNote', inpanel=False)\n    add.setXpos(r_marker.xpos()+315)\n    add.setYpos(r_marker.ypos()+15)\n    add.knob('tile_color').setValue(0x777777ff)\n    add.knob('note_font_size').setValue(20)\n    \n    # Placeholder\n    ph = nuke.createNode('Group', inpanel=False)\n    ph.knob('label').setValue(\"PLACEHOLDER - PAINT\")\n    ph.knob('tile_color').setValue(0xaa55ffff)\n    ph.setXpos(r_marker.xpos()+270)\n    ph.setYpos(r_marker.ypos()+256)\n    x_check = nuke.Boolean_Knob(\"Placeholder\", \"Placeholder\", True)\n    ph.addKnob(x_check)\n    \n    nuke.toNode(ph.knob('name').value()).begin()\n    ph_01 = nuke.createNode('Input', inpanel=False)\n    ph_01.knob('name').setValue(\"Plate\")\n    ph_02 = nuke.createNode('Input', inpanel=False)\n    ph_02.knob('name').setValue(\"Red\")\n    ph_03 = nuke.createNode('Input', inpanel=False)\n    ph_03.knob('name').setValue(\"Green\")\n    ph_04 = nuke.createNode('Input', inpanel=False)\n    ph_04.knob('name').setValue(\"Blue\")\n    ph_06 = nuke.createNode('Input', inpanel=False)\n    ph_06.knob('name').setValue(\"Description\")\n    ph_05 = nuke.createNode('Input', inpanel=False)\n    ph_05.knob('name').setValue(\"Paint\")\n    nuke.createNode('Output', inpanel=False)\n    ph.end()\n    \n    # RotoPaint nodes\n    rp_paint = nuke.createNode('RotoPaint', inpanel=False)\n    rp_paint.setXpos(r_marker.xpos()+100)\n    rp_paint.setYpos(r_marker.ypos()+250)\n    rp_paint.setInput(0, None)\n    rp_paint.knob('tile_color').setValue(0xffff00ff)\n    rp_paint.knob('label').setValue(\"PAINT\")\n    rp_paint.knob('output').setValue(\"alpha\")\n    rp_paint.knob('lifetime_type').setValue(\"all frames\")\n    \n    rp_white = nuke.createNode('RotoPaint', inpanel=False)\n    rp_white.setXpos(r_marker.xpos()+100)\n    rp_white.setYpos(r_marker.ypos()++330)\n    rp_white.setInput(0, None)\n    rp_white.knob('tile_color').setValue(0xffffffff)\n    rp_white.knob('label').setValue(\"DESCRIPTION\")\n    rp_white.knob('output').setValue(\"alpha\")\n    rp_white.knob('lifetime_type').setValue(\"all frames\")\n    \n    \n    dot_end = nuke.createNode('Dot', inpanel=False)\n    dot_end.setXpos(r_marker.xpos()+300)\n    dot_end.setYpos(r_marker.ypos()+720)\n    dot_end.setInput(0, ph)\n    \n    #Connect Placeholder\n    ph.setInput(0, dot)\n    #ph.setInput(1, rp_red)\n    #ph.setInput(2, rp_green)\n    #ph.setInput(3, rp_blue)\n    ph.setInput(5, rp_paint)\n    ph.setInput(4, rp_white)\n    \n    #r_marker.knob('name').setValue(\"old_Marker\")\n    #dot.knob('name').setValue(\"R_Marker\")\n    \n    #Counting amount of exisiting and additional Annotation added\n    switcher = nuke.toNode(\"Annotation_Switcher\")\n    p_count = int(switcher\['paintCounter'].value())+int(switcher\['a_pCounter'].value())\n    \n    new_Paint_no = int(p_count + 1)\n    new_Paint_name = \"paint\"\n    label = \"Paint \"\n    new_Paint = new_Paint_name + str(new_Paint_no)\n    new_Paint_label = label + str(new_Paint_no)\n    new = nuke.Boolean_Knob(new_Paint, new_Paint_label, True)\n    \n    #Add new Knob\n    switcher.addKnob(new)\n    paintCounter_no = int(switcher\['paintCounter'].value())\n    a_paintCounter = new_Paint_no-paintCounter_no\n    switcher\['a_pCounter'].setValue(a_paintCounter)\n    \n    #Linking to Append Clip and Contact Sheets\n    total_count = int(switcher\['total_Counter'].value())-1\n    total_paint_count = new_Paint_no-1\n    \n    appendClip = nuke.toNode(\"AppendClip\")\n    appendClip.setInput(total_count, dot_end)\n    \n    contactSheet_paint = nuke.toNode(\"SmartContactSheet_paint\")\n    contactSheet_paint.setInput(total_paint_count, dot_end)\n    \n    dot_suffix = \"_DOT\"\n    dot_name = new_Paint + dot_suffix \n    dot_end\['name'].setValue(dot_name)\n    \n    #Setting Sticky Note Label Name\n    add.knob('label').setValue(\"Additional- \"+new_Paint_label)\n    \n    #Update Sort Order Value\n    old_sortOrder = switcher\['sortOrder'].value()\n    new_sortOrder = new_Paint + \",\" + old_sortOrder\n    switcher\['sortOrder'].setValue(new_sortOrder)\n    \n    #Renaming r_Marker\n    r_marker_suffix = 'r_Marker_old'\n    r_marker_no = str(total_count)\n    r_marker_old = r_marker_suffix + r_marker_no\n    r_marker\['name'].setValue(r_marker_old)\n    \n    connector\['name'].setValue('r_Marker')\n    \n    del p_count\n    del new_Paint_no\n    del a_paintCounter\n    \n    del connector\n    del r_marker\n    \nelse:\n    nuke.message(\"There are still Paint Annotation Branches available!\")"}
