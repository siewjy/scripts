  addUserKnob {22 importPaint_custom l "<b>Import Custom Presets" T "n = nuke.thisNode()\n\n#Presets for all Shows\np_preset0 = \"Remove\"\np_preset1 = \"Remove all Markers\"\np_preset2 = \"Remove Wires\"\np_preset3 = \"Remove Seams\"\np_preset4 = \"Remove Rig\"\np_preset5 = \"Remove Stunt Actor\"\n\n#Custom Presets for Leads\np_inputCustom1 = n\['p_custom_01'].value()\np_inputCustom2 = n\['p_custom_02'].value()\np_inputCustom3 = n\['p_custom_03'].value()\np_inputCustom4 = n\['p_custom_04'].value()\np_inputCustom5 = n\['p_custom_05'].value()\np_inputCustom6 = n\['p_custom_06'].value()\np_inputCustom7 = n\['p_custom_07'].value()\np_inputCustom8 = n\['p_custom_08'].value()\np_inputCustom9 = n\['p_custom_09'].value()\np_inputCustom10 = n\['p_custom_10'].value()\n\ndef set_Paint_Customs():\n\n    for a in nuke.allNodes(\"Group\"):\n        task = a.knob(\"task_dropdown\")\n        if not task:\n            continue\n        \n        if task.value() == \"Paint\":\n            #a\['note_font_size'].setValue(15)\n            #Updating the Lists\n            a\['paint_presets'].setValues(\[p_preset0,p_preset1,p_preset2,p_preset3,p_preset4,p_preset5,p_inputCustom1,p_inputCustom2,p_inputCustom3,p_inputCustom4,p_inputCustom5,p_inputCustom6,p_inputCustom7,p_inputCustom8,p_inputCustom9,p_inputCustom10])\n           \nset_Paint_Customs()\n\n#Prompt to show Imported Customs\nnuke.message(\"<b>Paint Presets</b>\"+\"\\n\"+\"\\n\"+p_inputCustom1+\"\\n\"+p_inputCustom2+\"\\n\"+p_inputCustom3+\"\\n\"+p_inputCustom4+\"\\n\"+p_inputCustom5+\"\\n\"+p_inputCustom6+\"\\n\"+p_inputCustom7+\"\\n\"+p_inputCustom8+\"\\n\"+p_inputCustom9+\"\\n\"+p_inputCustom10+'\\n'+'\\n'+\"are imported!\")" +STARTLINE}
  addUserKnob {22 resetPaint_presets l "Reset Custom Presets" T "n = nuke.thisNode()\n\n#Presets for all Shows\np_p_preset0 = \"Remove\"\np_p_preset1 = \"Remove all Markers\"\np_p_preset2 = \"Remove Wires\"\np_p_preset3 = \"Remove Seams\"\np_p_preset4 = \"Remove Rig\"\np_p_preset5 = \"Remove Stunt Actor\"\n\nblank = \"\"\n\nn\['p_custom_01'].setValue(blank)\nn\['p_custom_02'].setValue(blank)\nn\['p_custom_03'].setValue(blank)\nn\['p_custom_04'].setValue(blank)\nn\['p_custom_05'].setValue(blank)\nn\['p_custom_06'].setValue(blank)\nn\['p_custom_07'].setValue(blank)\nn\['p_custom_08'].setValue(blank)\nn\['p_custom_09'].setValue(blank)\nn\['p_custom_10'].setValue(blank)\n\ndef reset_Customs():\n\n    for a in nuke.allNodes(\"Group\"):\n        task = a.knob(\"task_dropdown\")\n        if not task:\n            continue\n        \n        if task.value() == \"Paint\":\n            #a\['note_font_size'].setValue(15)\n            #Updating the Lists\n            a\['paint_presets'].setValues(\[p_preset0,p_preset1,p_preset2,p_preset3,p_preset4,p_preset5])\n            \n    nuke.message(\"Paint Custom List Cleared!\")\n        \nreset_Customs()" +STARTLINE}
